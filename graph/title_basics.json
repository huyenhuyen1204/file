{  
   "source":{  
      "file":{  
         "path":"/var/lib/mysql-files/title_basics.csv"
      }
   },
  "extractor": {
    "csv": {}
   },
   "transformers": [
	  {
	  "field": {
	    "fieldName": "@class",
	    "value": "TitleBasics"
	  },
	  { "vertex": { "class": "TitleBasics" } },
	  { "edge": { "class": "HasRatings",  "joinFieldName": "tconst", "lookup": "TitleBasics.tconst" } }

  }],
   "loader":{  
      "orientdb":{  
         "dbURL":"plocal:/opt/orientdb/databases/orientdb",
         "dbType":"graph",
        "dbUser": "admin",
        "dbPassword": "admin",
        "dbAutoCreate":true,
        "batchCommit": 1000,
        "wal":  false,
        "tx": false,
	
         "classes":[  
            {  
               "name":"TitleBasics",
	       "extends": "V"
            },
	     {"name": "HasRatings", "extends": "E"}
         ],
	"indexes":[   
	    {"class": "TitleBasics", "fields":["tconst:String"], "type":"UNIQUE_HASH_INDEX"}
         ]
      }
    }
}

		
